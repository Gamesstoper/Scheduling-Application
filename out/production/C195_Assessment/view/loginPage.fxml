<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.loginPage">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="passwordLabel" layoutX="127.0" layoutY="204.0" />
            <Button fx:id="loginButton" layoutX="278.0" layoutY="256.0" mnemonicParsing="false" onAction="#loginAction" />
            <TextField fx:id="UsernameTextField" layoutX="229.0" layoutY="152.0" onAction="#UserNameTextFieldHandler" />
            <PasswordField fx:id="PasswordTextField" layoutX="230.0" layoutY="200.0" onAction="#PasswordTextFieldHandler" />
            <Button fx:id="exitButton" layoutX="383.0" layoutY="256.0" mnemonicParsing="false" onAction="#exitAction" />
            <Label fx:id="usernameLabel" layoutX="127.0" layoutY="156.0" />
            <Label fx:id="titleLabel" layoutX="147.0" layoutY="14.0" prefHeight="56.0" prefWidth="262.0">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label fx:id="locationLabel" layoutX="100.0" layoutY="97.0" prefHeight="27.0" prefWidth="94.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="currentLocationLabel" layoutX="208.0" layoutY="97.0" prefHeight="27.0" prefWidth="224.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
